# Firebase Configuration (for React Native Firebase, these are set in google-services.json and GoogleService-Info.plist)
# But we need the Google Web Client ID for Google Sign-In
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your-google-web-client-id

# Firebase Web SDK Configuration (public keys - safe to expose in client)
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_web_api_key_here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id_here
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project_id.firebasestorage.app
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
EXPO_PUBLIC_FIREBASE_APP_ID=your_firebase_app_id_here

# AI API Keys (to be configured later)
EXPO_PUBLIC_OPENAI_API_KEY=your-openai-api-key
EXPO_PUBLIC_STABILITY_API_KEY=your-stability-api-key
EXPO_PUBLIC_ELEVEN_LABS_API_KEY=your-eleven-labs-api-key